      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic",
        "x-max-priority": 1
      }
    },
    {
      "name": "cleanup-temp-queue", 
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic",
        "x-message-ttl": 3600000
      }
    },
    {
      "name": "analytics-queue",
      "vhost": "/",
